{
  "name": "recruitment-system-ts",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "cd server && npm run start",
    "server": "cd server && npm run server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "description": "Connector creacted by ITGalkowski",
  "author": "Pawel Galkowski",
  "dependencies": {
    "concurrently": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocksdevil/apka.git"
  },
  "bugs": {
    "url": "https://github.com/rocksdevil/apka/issues"
  },
  "homepage": "https://github.com/rocksdevil/apka#readme"
}
